{
  "name": "@coloop-ai/temporal-superjson-payload-converter",
  "version": "0.1.0",
  "description": "SuperJSON-based payload converter for Temporal workflows. Properly serialize and deserialize complex types like Date, Map, Set, and BigInt.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Genei-Ltd/temporal-superjson-payload-converter.git"
  },
  "bugs": {
    "url": "https://github.com/Genei-Ltd/temporal-superjson-payload-converter/issues"
  },
  "homepage": "https://github.com/Genei-Ltd/temporal-superjson-payload-converter#readme",
  "keywords": [
    "temporal",
    "temporal.io",
    "superjson",
    "payload-converter",
    "serialization",
    "workflow",
    "activities"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run tc && npm run lint && npm run test -- --run && npm run build",
    "tc": "tsc -p tsconfig.json --pretty --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "superjson": "^2.0.0"
  },
  "peerDependencies": {
    "@temporalio/common": "^1.0.0"
  },
  "devDependencies": {
    "@temporalio/common": "^1.11.7",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "globals": "^15.0.0",
    "prettier": "^3.0.0",
    "tsdown": "~0.18.0",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
